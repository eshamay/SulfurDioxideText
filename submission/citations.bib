@ARTICLE{Baltrusaitis2011,
  author = {Baltrusaitis, J. and Jayaweera, P. M. and Grassian, V. H.},
  title = {Sulfur Dioxide Adsorption on TiO2 Nanoparticles: Influence of Particle
	Size, Coadsorbates, Sample Pretreatment, and Light on Surface Speciation
